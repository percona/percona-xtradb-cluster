# xbstream+compress+parallel+compress-threads+compact+compress-stream+encrypt2+rlimit+time+progress
# v2only
[xtrabackup]
compress
parallel=4
compress-threads=4
compact

[sst]
streamfmt=xbstream
compressor="gzip"
encrypt=2
tca=/tmp/certs/node1.crt
tcert=/tmp/certs/node2.pem
rlimit=2m
time=1
progress=1
