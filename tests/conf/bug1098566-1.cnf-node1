# compress+parallel+compress-stream+sst-special-dirs+encrypt2

[mysqld]
innodb_data_home_dir=/tmp/var-test/data1
innodb_log_group_home_dir=/tmp/var-test/log1

[xtrabackup]
compress
parallel=4
compress-threads=4


[sst]
compressor="gzip"
streamfmt=xbstream
sst-special-dirs=1
encrypt=2
tca=/tmp/certs/node1.crt
tcert=/tmp/certs/node1.pem
