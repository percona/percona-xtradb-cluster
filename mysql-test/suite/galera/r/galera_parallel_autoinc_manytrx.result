CREATE TABLE t1 (f1 INTEGER AUTO_INCREMENT PRIMARY KEY, f2 INTEGER) Engine=InnoDB;
set session wsrep_sync_wait=15;
SET GLOBAL wsrep_slave_threads = 4;
Timeout in wait_condition.inc for SELECT VARIABLE_VALUE = 4 FROM INFORMATION_SCHEMA.GLOBAL_STATUS WHERE VARIABLE_NAME = 'wsrep_applier_thread_count';
CREATE PROCEDURE p1 (repeat_count int)
BEGIN
DECLARE current_num int;
SET current_num = 0;
WHILE current_num < repeat_count do
INSERT INTO t1 (f2) SELECT 1 FROM ten AS a1;
COMMIT;
SET current_num = current_num + 1;
END WHILE;
END|
